{
  "name": "fitcoachai",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "workspaces": [
    "frontend/*",
    "frontend/RealtorInterface/*"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build -c backend/nest-cli.json -p backend/tsconfig.build.json",
    "format": "prettier --write \"backend/src/**/*.ts\" \"backend/test/**/*.ts\"",
    "start": "nest start -c backend/nest-cli.json -p backend/tsconfig.build.json",
    "start:dev": "nest start --watch -c backend/nest-cli.json -p backend/tsconfig.build.json",
    "start:debug": "nest start --debug --watch -c backend/nest-cli.json -p backend/tsconfig.build.json",
    "start:prod": "node backend/dist/main",
    "lint": "eslint \"{backend/src,frontend,libs,backend/test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/serve-static": "^5.0.3",
    "@supabase/supabase-js": "^2.49.8",
    "bullmq": "^5.51.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "ioredis": "^5.6.1",
    "luxon": "^3.6.1",
    "openai": "^4.96.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "postcss-color-rgb": "^2.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vitest": "^1.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "backend/src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "type": "module",
  "packageManager": "pnpm@8.6.10"
}
